<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    style="@style/textview4_style"
    android:gravity="center"
    android:id="@+id/webview_error"
    android:visibility="gone"
    >
        <LinearLayout
            android:id="@+id/webview_onclick"
            style="@style/textview1_style"
            android:orientation="vertical"
            android:gravity="center"
            >
        <ImageView
            style="@style/textview1_style"
            android:src="@mipmap/cs_tx"
            />
        <TextView
            style="@style/textview1_style"
            android:text="@string/error_no_data"
            android:layout_marginTop="35px"
            android:textColor="@android:color/black"
            android:textSize="@dimen/siez_dime5"
            />
        <TextView
            style="@style/textview1_style"
            android:text="@string/no_network_click_loading"
            android:layout_marginTop="25px"
            android:textSize="@dimen/siez_dime3"
            />
        </LinearLayout>
</LinearLayout>